{
  "version_source": "hopalongv124.html",
  "formulas": [
    {
      "id": "classic_sqrt",
      "name": "Classic (sqrt)",
      "desc": "Wolfram/Martin: x' = y − sgn(x)·sqrt(|βx − δ|), y' = α − x"
    },
    {
      "id": "sqrt_plus_gamma_y",
      "name": "Classic + γy",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γy, y' = α − x"
    },
    {
      "id": "sqrt_plus_gamma_x",
      "name": "Classic + γx",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γx, y' = α − x"
    },
    {
      "id": "mix_inside",
      "name": "sqrt(|β(x+γy)−δ|)",
      "desc": "Mix x,y inside sqrt: x' = y − sgn(x)·sqrt(|β(x+γy) − δ|), y' = α − x"
    },
    {
      "id": "trig_kick_x",
      "name": "Trig kick (sin x)",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γ·sin(x), y' = α − x"
    },
    {
      "id": "damped",
      "name": "Damped (|d|)",
      "desc": "x' = (y − sgn(x)·sqrt(|b x − c|))·(1−|d|), y' = a − x"
    },
    {
      "id": "y_feedback",
      "name": "Y feedback (a−x+d·y)",
      "desc": "Keep classic x', add feedback on y: y' = a − x + d·y"
    },
    {
      "id": "trig_kick_y",
      "name": "Trig kick (sin y)",
      "desc": "x' = y − sgn(x)·sqrt(|b x − c|) + d·sin(y), y' = a − x"
    },
    {
      "id": "classic_plus_yy",
      "name": "Classic + d·y²",
      "desc": "x' = y − sgn(x)·sqrt(|b x − c|) + d·y², y' = a − x"
    },
    {
      "id": "cos_xy_kick",
      "name": "Trig kick (cos(x+y))",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γ·cos(x+y), y' = α − x"
    },
    {
      "id": "inside_sin_y",
      "name": "Inside sqrt: x+γ·sin(y)",
      "desc": "x' = y − sgn(x)·sqrt(|β(x+γ·sin(y)) − δ|), y' = α − x"
    },
    {
      "id": "inside_cos_x",
      "name": "Inside sqrt: x+γ·cos(x)",
      "desc": "x' = y − sgn(x)·sqrt(|β(x+γ·cos(x)) − δ|), y' = α − x"
    },
    {
      "id": "softsign_kick",
      "name": "Softsign kick",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γ·(x/(1+|x|)), y' = α − x"
    },
    {
      "id": "tanh_kick",
      "name": "Tanh kick",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γ·tanh(x), y' = α − x"
    },
    {
      "id": "sign_xy",
      "name": "Sign of (x·y)",
      "desc": "Use sign(x·y): x' = y − sgn(xy)·sqrt(|βx − δ|), y' = α − x"
    },
    {
      "id": "double_root",
      "name": "Double-root kick",
      "desc": "x' = y − sgn(x)·(sqrt(|βx−δ|)+γ·sqrt(|βy−δ|)), y' = α − x"
    },
    {
      "id": "xy_coupling",
      "name": "XY coupling",
      "desc": "x' = y − sgn(x)·sqrt(|βx − δ|) + γ·(x·y/50), y' = α − x"
    },
    {
      "id": "positive_hopalong",
      "name": "Positive Hopalong (+sqrt)",
      "desc": "x' = y + sgn(x)·sqrt(|βx − δ|), y' = α − x"
    },
    {
      "id": "sinusoidal_hopalong",
      "name": "Sinusoidal Hopalong (sin)",
      "desc": "x' = y + sin(βx − δ), y' = α − x"
    },
    {
      "id": "threeply",
      "name": "Threeply (Peters)",
      "desc": "x' = y − sgn(x)·|sin(x)cos(β) + γ − x·sin(α+β+γ)|, y' = α − x"
    },
    {
      "id": "quadrup2",
      "name": "Quadrup-2 (Peters)",
      "desc": "x' = y − sgn(x)·sin(ln|βx−δ|)·atan((ln|δx−β|)^2), y' = α − x"
    },
    {
      "id": "chip",
      "name": "Chip (Peters)",
      "desc": "x' = y − sgn(x)·cos((ln|βx−δ|)^2)·atan((ln|βx−δ|)^2), y' = α − x"
    }
  ],
